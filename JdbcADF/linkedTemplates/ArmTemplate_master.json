{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "JdbcADF"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"ls_MySql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_MySql'"
		},
		"ls_jdbc_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_jdbc'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sskstorage9.dfs.core.windows.net/"
		},
		"ls_MySql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "127.0.0.1"
		},
		"ls_MySql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ssk"
		},
		"ls_MySql_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "root"
		},
		"ls_jdbc_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1220-062953-76qyn9n7"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "JdbcADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"ls_jdbc_accessToken": {
						"value": "[parameters('ls_jdbc_accessToken')]"
					},
					"ls_MySql_password": {
						"value": "[parameters('ls_MySql_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_jdbc_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_jdbc_properties_typeProperties_existingClusterId')]"
					},
					"ls_MySql_properties_typeProperties_server": {
						"value": "[parameters('ls_MySql_properties_typeProperties_server')]"
					},
					"ls_MySql_properties_typeProperties_database": {
						"value": "[parameters('ls_MySql_properties_typeProperties_database')]"
					},
					"ls_MySql_properties_typeProperties_username": {
						"value": "[parameters('ls_MySql_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}